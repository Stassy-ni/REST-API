{
  "type": "object",
  "description": "A higher-level category that groups topics within a subject and grade range.\n",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Unique ID of the section.",
      "example": 301
    },
    "subject": {
      "type": "string",
      "description": "Subject of the section.",
      "example": "math"
    },
    "grade_range": {
      "type": "string",
      "description": "Grades covered by the section.",
      "example": "5-6"
    },
    "title": {
      "type": "string",
      "description": "Title of the section.",
      "example": "\u0410\u0440\u0438\u0444\u043c\u0435\u0442\u0438\u043a\u0430 \u0434\u0440\u043e\u0431\u0435\u0439"
    }
  },
  "required": [
    "id",
    "subject",
    "grade_range",
    "title"
  ]
}
